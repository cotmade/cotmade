platform :ios, '13.0'

target 'Runner' do
  use_frameworks!
  use_modular_headers!

  # Flutter dependencies
  pod 'Flutter', :path => File.expand_path('..')

  # Firebase dependencies (add more based on the plugins you're using)
  pod 'Firebase/Core'
  pod 'GoogleMaps'

  # Other dependencies (e.g., cloud_firestore, firebase_auth, etc.)
  pod 'cloud_firestore'
  pod 'firebase_auth'
  pod 'firebase_messaging'
  pod 'firebase_storage'

  target 'RunnerTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
